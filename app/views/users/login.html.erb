


<div class="mx-auto rounded login-wrapper shadow-lg">
<div class="discription">
<%= image_tag('logo.png' , class: 'logo') %>
  <p class="p-0 m-0">ユーザー名: test</p>
  <p class="p-0 mb-2">パスワード: testtest　でログインできます。</p>

</div>
<%= form_for(:session, method: :post, url: s_create_path) do |f| %>
  <div class="form-group">
    <%= f.text_field :name, class: 'form-control', placeholder: "ユーザー名" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, class: 'form-control',placeholder: "パスワード" %>
  </div>
  <%= f.submit 'ログイン', class: 'mt-2 btn btn-login btn-block mx-auto d-block' %>
<% end %>
</div>